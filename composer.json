{
  "name": "mez/zf-doctrine-behaviors",
  "description": "Doctrine Behaviors ported to Zend Framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin Meredith",
      "email": "martin@sourceguru.net"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-intl": "^7.3",
    "doctrine/dbal": "^2.9",
    "knplabs/doctrine-behaviors": "^1.6",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "jakub-onderka/php-parallel-lint": "^1.0",
    "localheinz/composer-normalize": "^1.3",
    "maglnet/composer-require-checker": "^2.0",
    "nikic/php-parser": "^4.2",
    "phpmd/phpmd": "^2.7",
    "phpro/grumphp": "^0.17.0",
    "phpstan/phpstan": "^0.12.0",
    "povils/phpmnd": "^2.1",
    "sebastian/phpcpd": "^4.1",
    "sensiolabs/security-checker": "^6.0",
    "zendframework/zend-servicemanager": "^3.4"
  },
  "config": {
    "platform": {
      "php": "7.3"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Mez\\DoctrineBehaviors\\": [
        "src"
      ]
    }
  },
  "scripts": {
    "cs-check": "php-cs-fixer --allow-risky=yes --config=.php_cs --verbose --dry-run fix",
    "cs-fix": "php-cs-fixer --allow-risky=yes --config=.php_cs --verbose fix"
  }
}
